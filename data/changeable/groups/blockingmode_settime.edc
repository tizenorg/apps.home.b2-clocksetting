            }
            align: 1.0 0.5;
         }
      }
      part { name: "start_upbtn";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 0;
            min: 72 0;
            max: 72 -1;
            rel1 {
               relative: 1.0 0.0;
               to_x: "start_upbtn.left.padding";
               to_y: "upbtn.field";
            }
            rel2 {
               relative: 1.0 1.0;
               to_x: "start_upbtn.left.padding";
               to_y: "upbtn.field";
            }
            align: 0.0 0.5;
            visible: 1;
         }
	  }
      part { name: "start_downbtn";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 0;
            min: 72 0;
            max: 72 -1;
            rel1 {
               relative: 1.0 0.0;
               to_x: "start_upbtn.left.padding";
               to_y: "downbtn.field";
            }
            rel2 {
               relative: 1.0 1.0;
               to_x: "start_upbtn.left.padding";
               to_y: "downbtn.field";
            }
            align: 0.0 0.5;
            visible: 1;
         }
	  }

      part { name: "start_hour";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 0;
            min: 72 0;
            max: 72 -1;
            rel1 {
               relative: 1.0 0.0;
               to_x: "left.padding";
               to_y: "content.field";
            }
            rel2 {
               relative: 1.0 1.0;
               to_x: "left.padding";
               to_y: "content.field";
            }
            align: 0.0 0.5;
            visible: 1;
         }
	  }
      part { name: "start_hour.ampm.seperator";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 0;
            min: 10 0;
            max: 10 -1;
            rel1 {

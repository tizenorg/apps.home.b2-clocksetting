               to_y: "downbtn.upper.seperator";
            }
            align: 0.5 0.0;
         }
      }
      part { name: "start_upbtn.left.padding";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 0;
            min: 0 0;
            max: 0 -1;
            rel1 {
               relative: 1.0 0.0;
               to: "left.padding";
			}
            rel2 {
               relative: 1.0 1.0;
               to: "left.padding";
            }
            align: 0.0 0.5;
         }
      }
      part { name: "start_upbtn.right.padding";
         type: SPACER;
         scale: 1;
